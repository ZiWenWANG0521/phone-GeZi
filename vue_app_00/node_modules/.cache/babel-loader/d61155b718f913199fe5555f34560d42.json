ock;\r\n        border-bottom:rgba(0,0,0,.2) solid 1px;\r\n        width:100%;\r\n    }\r\n    .mui-user{\r\n        display:flex;\r\n        justify-content: space-between;\r\n    }\r\n    .mui-user .mui-user-msg,.mui-icon-eye{\r\n            line-height:70px;\r\n    }\r\n    .mui-user .mui-user-msg{\r\n            color:rgba(0,0,0,.4);\r\n            padding-left: 8px;\r\n    }\r\n    .mui-user .mui-user-msg .mui-icon-contact{\r\n        color:lime;\r\n    }\r\n    .mui-user .mui-icon-eye{\r\n            color:rgba(0,0,0,.4);\r\n            font-size:12px;\r\n            padding-right: 8px;\r\n    }\r\n    .img-show-list{\r\n        width:100%;\r\n        height:200px;\r\n        background: #fff;\r\n        box-shadow: 0 1px 1px rgba(0, 0, 0, .2);\r\n       padding-top:10px;\r\n    }\r\n    .img-show-list .img-list-title{\r\n        color:black;\r\n        font-size:17px;\r\n        font-weight: bold;\r\n    }\r\n    .img-show-list .img-list-item{\r\n        width:100%;\r\n        height:180px;\r\n        overflow-x:auto;\r\n        white-space: nowrap;\r\n    }\r\n    .img-show-list .img-list-item .item{\r\n        width:200px;\r\n        height:150px;\r\n        display: inline-block;\r\n        margin-right:10px;\r\n    }\r\n    .img-show-list .img-list-item .item h5{\r\n        white-space: nowrap;\r\n        text-overflow: ellipsis;\r\n        overflow: hidden;\r\n        font-weight: bold;\r\n        color:black;\r\n        font-style: normal;\r\n        padding-left: 2px;\r\n    }\r\n    .img-show-list .img-list-item img{\r\n        width:200px;\r\n        height:100px;\r\n    }\r\n    .img-show-list .img-list-item .list-msg{\r\n        white-space: nowrap;\r\n        text-overflow: ellipsis;\r\n        width:200px;\r\n        height:20px;\r\n        overflow: hidden;\r\n        font-weight: normal;\r\n        padding-left: 2px;\r\n    }\r\n    .mui-card-content-inner .mui-msg-content{\r\n        width:165px;\r\n        float:left;\r\n    }\r\n    .mui-card-content-inner .mui-msg-content img[lazy=loading]{\r\n        height:150px;\r\n    }\r\n    .mui-card-content-inner .mui-msg-content .mui-content-title{\r\n        font-size:15px;\r\n        color:black;\r\n        margin-bottom: 0;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        white-space:nowrap;\r\n    }\r\n    .mui-card-content-inner .mui-msg-content .content-msg{\r\n        white-space: nowrap;\r\n        text-overflow: ellipsis;\r\n        overflow: hidden;\r\n        color:black;\r\n    }\r\n    .mui-card-content-inner .mui-msg-content .content-price .price{\r\n        color:#9bb26c;\r\n        font-size:16px;\r\n    }\r\n    .mui-card-content-inner .mui-msg-content .content-price .price-old{\r\n        font-size: 1px;\r\n        text-decoration: line-through;\r\n        margin-left: 5px;\r\n    }\r\n</style>"],"sourceRoot":"src\\components\\tabbar"}]}