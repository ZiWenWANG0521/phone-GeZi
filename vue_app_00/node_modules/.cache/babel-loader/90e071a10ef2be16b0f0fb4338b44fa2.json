       <div class=\"mui-card-content-inner\">\r\n                   <swiper-box :list=\"imglist\"></swiper-box>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!--商品购买区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">{{info.title}}</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                  市场价: <s>{{info.old}}</s> 销售价: <span>{{info.now}}</span>\r\n                    <p>\r\n                    购买数量\r\n                      <div class=\"mui-numbox\">\r\n                        <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"aa\">-</button>\r\n                        <input class=\"mui-input-numbox\" type=\"number\" value=\"1\" v-model=\"val\"/>\r\n                        <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"bb\">+</button>\r\n                      </div>\r\n                    </p>\r\n                    <div>\r\n                        <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\">立即购买</button>\r\n                        <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"addCartTo\">加入购物车</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!--商品参数区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">商品参数</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <p>商品货号:11</p>\r\n                    <p>商品货号:121</p>\r\n                    <p>商品货号:13</p>\r\n                    <p>商品货号:101</p>\r\n                </div>\r\n            </div>\r\n            <div class=\"mui-card-footer\">\r\n                <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\">图文介绍</button>\r\n                <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\">商品评论</button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import swiper from \"../sub/swiper.vue\"\r\n    import {Toast} from \"mint-ui\"\r\n   // import mui from \"../../lib/mui/js/mui.js\"\r\n    export default {\r\n        name: \"GoodInfo\",\r\n\r\n    data(){\r\n        return {id:this.$route.params.id,imglist:[],val:\"1\",info:{}}\r\n    },\r\n        components:{\r\n          \"swiper-box\":swiper\r\n        },\r\n        methods:{\r\n            getImage(){\r\n                var url=\"imagelist\";\r\n                this.$http.get(url).then(result=>{\r\n                    this.imglist=result.body\r\n                })\r\n            },\r\n            aa(){\r\n                if(this.val>0)\r\n                this.val--;\r\n            },\r\n            bb(){\r\n                if(this.val<999)\r\n                this.val++;\r\n            },\r\n            getGoodInfo(){\r\n                this.$http.get(\"goodsinfo?id=\"+this.id).then(result=>{\r\n                    this.info=result.body\r\n                })\r\n            },\r\n            addCartTo(){\r\n                this.$http.get(\"addcart?pid=\"+this.id+\"&count=\"+this.val).then(result=>{\r\n                    if(result.body.msg == 1){\r\n                        this.$store.commit(\"increment\",this.val)\r\n                        Toast(result.body.msg)\r\n                    }else{\r\n                        Toast(result.body.msg);\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        created(){\r\n            this.getImage()\r\n            this.getGoodInfo()\r\n        },\r\n        mounted(){\r\n            this.aa()\r\n            this.bb()\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src\\components\\goods"}]}