     <!--轮播组件-->\r\n                    <swiper-box :list=\"imglist\"></swiper-box>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <!--\r\n                     <ul class=\"mui-table-view\">\r\n                               <li class=\"mui-table-view-cell mui-media\">\r\n                                   <a href=\"javascript:;\">\r\n                                     <img class=\"mui-media-object mui-pull-left\" src=\"\">\r\n                                     <div class=\"mui-media-body\">\r\n                                         幸福\r\n                                         <p class='mui-ellipsis'>能和心爱的人一起睡觉，是件幸福的事情；可是，打呼噜怎么办？</p>\r\n                                     </div>\r\n                                   </a>\r\n                               </li>\r\n                     </ul>\r\n                             -->\r\n                    <!--2.1mui 左侧图片 右侧文字-->\r\n                    <ul class=\"mui-table-view\">\r\n                        <li class=\"mui-table-view-cell mui-media\" v-for=\"item in list\" :key=\"item.id\">\r\n                            <a href=\"javascript:;\">\r\n                                <img class=\"mui-media-object mui-pull-left\" src=\"\">\r\n                                <div class=\"mui-media-body\">\r\n                                    {{item.title}}\r\n                                    <p class='mui-ellipsis'>\r\n                                        <span class=\"price\">{{item.price*item.count}}</span>\r\n                                        <span class=\"count\">\r\n                        <!--2.2mui 数字按钮-->\r\n                                            <div class=\"mui-numbox\" data-numbox-min='1' data-numbox-max='9'>\r\n                          <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"shopS(item.id)\">-</button>\r\n                          <input id=\"test\" class=\"mui-input-numbox\" type=\"number\" v-model=\"item.count\" />\r\n                          <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"shopAdd(item.id)\">+</button>\r\n                                           </div>\r\n                         </span>\r\n                                    </p>\r\n                                </div>\r\n                            </a>\r\n                        </li>\r\n                    </ul>\r\n                    <div>合计:{{getSubTotal}}</div>\r\n                 </div>\r\n              </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import swiper from \"../sub/swiper.vue\"\r\n    export default {\r\n        name: \"ShopContainer\",\r\n        data(){\r\n            return {imglist:\"\",list:[],count:\"\"}\r\n        },\r\n        components:{\r\n            \"swiper-box\":swiper\r\n        },\r\n        /*计算属性,元素变化时再执行*/\r\n        computed:{\r\n            getSubTotal(){\r\n                var add=0;\r\n                for(var item of this.list){\r\n                    add += item.price*item.count\r\n                }\r\n                return add\r\n            }\r\n        },\r\n        methods:{\r\n            getImage(){\r\n                var url=\"imagelist\";\r\n                this.$http.get(url).then(result=>{\r\n                    this.imglist=result.body\r\n                })\r\n            },\r\n            getInfo(){\r\n                var url=\"shopCart\";\r\n                this.$http.get(url).then(result=>{\r\n\r\n                    this.list=result.body\r\n\r\n                })\r\n            },\r\n            shopAdd(id){\r\n                /*循环中,点击一项加减不影响其他的,要在方法中传值,和双向绑定中写形参,做判断形参和方法中id相等,才执行加减操作*/\r\n                for(var item of this.list){\r\n                    if(id==item.id&&item.count<999)\r\n                        item.count++\r\n                }\r\n            },\r\n            shopS(id){\r\n                for(var item of this.list){\r\n                    if(id==item.id&&item.count>1)\r\n                        item.count--\r\n                }\r\n            }\r\n        },\r\n        created(){\r\n            this.getImage()\r\n            this.getInfo()\r\n            this.shopAdd()\r\n            this.shopS()\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n  *{\r\n      padding:0;\r\n      margin:0;\r\n  }\r\n  .mui-card{\r\n   width:500px;\r\n\r\n}\r\n    .mui-numbox{\r\n        margin-left: 100px;\r\n    }\r\n</style>"]}]}